<% provide(:title, "Sign in") %>
<h1>登录</h1>
 <%= form_for(:session, url: sessions_path) do |f| %>

      <%= f.label '电子邮箱' %>
      <%= f.text_field :email %>

      <%= f.label '密码' %>
      <%= f.password_field :password %>

      <%= f.submit "登录", class: "btn btn-large btn-primary" %>
    <% end %>
    
 <p>没有账号? <%= link_to "立即注册", signup_path %></p>