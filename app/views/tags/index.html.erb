<h1>Listing tags</h1>
<%= StuffsTag.tags_gruops current_user %>
<table>
  <tbody>
    <% @tags.each do |tag| %>
      <tr>
        <td><%= link_to tag.name, tag %>

        </td>
        <td><%= link_to 'Edit', edit_tag_path(tag) %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tag', new_tag_path %>
